{
  "name": "polarizetech/dispatch",
  "description": "Shipping calculations and label generation for Laravel",
  "version": "0.0.3",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.2"
  },
  "autoload": {
      "psr-4": {
          "Dispatch\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Dispatch\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Dispatch\\DispatchServiceProvider"
          ]
      }
  },
  "require-dev": {
    "orchestra/testbench": "^9.0"
  }
}
